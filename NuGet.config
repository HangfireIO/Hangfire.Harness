<configuration>
    <packageSources>
        <add key="HangfirePro" value="https://nuget.hangfire.io/nuget/hangfire-pro/" />
        <add key="HangfireCI" value="https://nuget.hangfire.io/nuget/hangfire-ci/" />
    </packageSources>
    <packageSourceCredentials>
        <HangfirePro>
            <add key="Username" value="%HANGFIRE_LOGIN%" />
            <add key="ClearTextPassword" value="%HANGFIRE_PASSWORD%" />
        </HangfirePro>
        <HangfireCI>
            <add key="Username" value="%HANGFIRE_LOGIN%" />
            <add key="ClearTextPassword" value="%HANGFIRE_PASSWORD%" />
        </HangfireCI>
    </packageSourceCredentials>
</configuration>
